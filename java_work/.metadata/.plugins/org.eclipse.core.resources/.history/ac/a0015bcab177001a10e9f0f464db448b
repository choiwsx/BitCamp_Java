package org.wonseok.pt1;

import java.util.Hashtable;

public class Ex03_Map {
	public static void main(String[] args) {
		// map 인터페이스를 상속 받아서 -> HashTable 컬렉션 클래스
		// Key : Value 의 쌍으로 요소가 구성된다.
		Hashtable<String, String> map = new Hashtable<>();
		map.put("홍길동", "010-1111-1111");
		map.put("김길동", "010-2222-1111");
		map.put("홍길순", "010-3333-1111");
		map.put("박길동", "010-4444-1111");
		
		System.out.println(map);
	}
}
