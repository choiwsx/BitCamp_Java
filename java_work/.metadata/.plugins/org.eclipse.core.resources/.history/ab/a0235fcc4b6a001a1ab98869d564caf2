package org.wonseok;

public class Ch02EX14 {
	
	static final int MAX = 10;

	
	public static void main(String[] args) {
		
		int prev = 1;
		int cur = 1;
		int next = 0;
		
		for(;cur<=21;)
		{
			System.out.println(cur);
			next = prev + cur;
			prev = cur;
			cur = next;
			
		}
		
	}
	
	
	public static void test01(String[] args) {
		// 
		
		int total = 2; 
		for(int i=1; i<=MAX; i++)
		{
			System.out.print(i);
			if(i%2==0)
			{
				System.out.print(i==MAX?"=":"-");
				total += i ;
			}
			else{
				System.out.print("+");		
				total -= i;
			}
		}
		
		System.out.printf("%d",total);
	}
}
