package org.wonseok.controller;


public class TT {

	public static void main(String[] args) {
		Thread th;
		th = new Thread(TT);
		th.start();
		
		
	}
	private void run() {
		while(true)
		{
			for(int i=1; i<=10; i++)
				System.out.println(i);
			try {
				Thread.sleep(3000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
