package org.wonseok;

class Point1
{
	int x;
	int y;
	
	@Override
	public String toString() {
		return "Point1 [x=" + x + ", y=" + y + "]";
	}
	
}


public class Ch04Ex02 {
	public static void main(String[] args) {
		Point1 pt = new Point1();
		pt.x = 100;
		pt.y = 200;
		System.out.println(pt);
		
		
	}
}
