package org.wonseok.ch04_1;


class Test{
	public void printArr(int[] arr){
		for(int i=0; i<arr.length; i++)
		{
			System.out.print(arr[i]+" ");
		}
	}
}


public class Ch04Ex07 {

	public static void main(String[] args) {
		int[] arr = {10,20,30,40,50};
		
		Test test = new Test();
		test.printArr(arr);
	}
}
