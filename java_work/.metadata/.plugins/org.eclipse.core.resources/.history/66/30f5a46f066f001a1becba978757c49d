package org.wonseok;



public class Ch04Ex04 {
	
	
	public void print(int ... a)
	{
		System.out.println("프린트1 호출");
		for(int i=0; i<a.length; i++)
		{
			System.out.println(a[i]);
		}
	}

	
	public static void main(String[] args) {
	
		Ch04Ex04 obj = new Ch04Ex04();
		obj.print(100);
		obj.print(1,2);
		obj.print(-1,-2,-3);
	}
}
