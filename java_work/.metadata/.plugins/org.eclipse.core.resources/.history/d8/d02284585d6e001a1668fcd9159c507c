package org.wonseok;

import java.util.Scanner;

public class Ch03Ex35 {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int num = 100;
		int div = 0;
		
		System.out.print("나눗수를 입력하시오 > ");
		div = scan.nextInt();
//		System.out.print("나눗수를 입력하시오 > ");
//		idv = scan.nextInt();
		System.out.printf("100을 %d로 나눈 몫->%d", div, num/div);
		
	}
}
