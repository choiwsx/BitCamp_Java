package org.wonseok;

public class mul {
	public static void main(String[] args) {
		
		for(int dan=2; dan<=9; dan+=3)
		{
			for(int i=0; i<3; i++)
			{
				if(dan+i!=10)
					System.out.printf("**%d**\t",dan+i);		
			}
			System.out.println();
			for(int cnt=1; cnt<=9; cnt++)
			{
				System.out.printf("%d*%d=%d",dan,cnt,dan*cnt);
				System.out.printf("%d*%d=%d",dan,cnt,dan*cnt);
				System.out.printf("%d*%d=%d",dan,cnt,dan*cnt);
				System.out.println();
			}
		}
	}
}
