package org.wonseok;

public class Point {
	int x,y;
	public Point() {
		// TODO Auto-generated constructor stub
		this(1,1);
	}
	public Point(int x, int y)
	{
		this.x = x;
		this.y = y;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "x="+x+",y="+y; 
	}
	
	static double getDistance(Point p1, Point p2)
	{
		int a = p1.x - p2.x;
		int b = p1.y - p2.y;
		
		double d = Math.sqrt(a*a+b*b);
		return d;
	}
}
