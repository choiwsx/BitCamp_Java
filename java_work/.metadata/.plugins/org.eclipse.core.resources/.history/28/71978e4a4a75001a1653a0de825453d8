package org.wonseok.controller;


import static org.wonseok.resource.R.*;

public class Controller {

	public void service() {
		// TODO Auto-generated method stub
		viewArr[data.number].display();
		
		switch(data.number)
		{
		case 1:
			viewArr[data.number].display();
			dao.insert(data.dto);
			break;
		case 2:
			viewArr[data.number].display();
			dao.selectAll(data.dtoArr);
			break;
			
			
		}
		data.number = 0;
		service();
		
		
	}

}
